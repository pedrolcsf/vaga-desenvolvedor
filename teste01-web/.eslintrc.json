{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "xo"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "object-curly-spacing": "off",
        "indent": "off",
        "complexity": "off",
        "no-unused-vars": "off",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "eqeqeq": "off",
        "radix": "off",
        "no-useless-escape": "off",
        "no-alert": "off",
        "camelcase": "off"
    }
}